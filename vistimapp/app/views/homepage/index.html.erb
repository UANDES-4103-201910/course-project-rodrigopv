    <div class="row">
      <div class="col-lg-12">
        <h1 class="mt-5 text-center">Bienvenide</h1>
        <p class="lead">Escribe tus quejas en este sitio!</p>
      </div>
    </div>
    <div class="row">
      <div class="col-md-8">
        <div class="card box-shadow">
            <div class="card-header">
                <h3 class="">Quejas Populares</h3>

            </div>
            <div class="card-body">
    <% @complaints.each do |complaint| %>
        <li><%= link_to complaint.title, complaint %> (author: <%= link_to complaint.user.name, complaint.user %>)</li>
    <% end %>

            </div>
        </div>
      </div>
      <div class="col-md-4">
        <div class="card box-shadow">
            <div class="card-header">
                <h3 class="text-center">Nuevos Usuarios</h3>
            </div>
            <div class="card-body">
              <ul>
    <% @users.each do |user| %>
        <li><%= link_to user.name, user %></li>
    <% end %>
              </ul>
            </div>
        </div>
      </div>

    </div>

